<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <CheckBox
        android:id="@+id/chkIconeExercicio"
        android:layout_width="@dimen/celulaWidth"
        android:layout_height="@dimen/celulaHeight"
        android:background="@drawable/ic_grafico_exercicio_chk"
        android:button="@null"
        android:clickable="false"
        tools:layout_editor_absoluteY="17dp"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="42dp"
        android:layout_alignParentStart="true" />

    <CheckBox
        android:id="@+id/checkBox1"
        android:layout_width="@dimen/celulaWidth"
        android:layout_height="@dimen/celulaHeight"
        android:background="@drawable/custom_selector"
        android:button="@null"
        android:clickable="false"
        app:layout_constraintStart_toEndOf="@+id/chkIconeExercicio"
        tools:layout_editor_absoluteY="17dp" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="@dimen/celulaWidth"
        android:layout_height="@dimen/celulaHeight"
        android:background="@drawable/custom_selector"
        android:button="@null"
        android:clickable="false"
        app:layout_constraintBottom_toBottomOf="@+id/checkBox1"
        app:layout_constraintStart_toEndOf="@+id/checkBox1" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="@dimen/celulaWidth"
        android:layout_height="@dimen/celulaHeight"
        android:background="@drawable/custom_selector"
        android:button="@null"
        android:clickable="false"
        app:layout_constraintBottom_toBottomOf="@+id/checkBox2"
        app:layout_constraintStart_toEndOf="@+id/checkBox2" />

    <CheckBox
        android:id="@+id/checkBox4"
        android:layout_width="@dimen/celulaWidth"
        android:layout_height="@dimen/celulaHeight"
        android:background="@drawable/custom_selector"
        android:button="@null"
        android:clickable="false"
        app:layout_constraintBottom_toBottomOf="@+id/checkBox3"
        app:layout_constraintStart_toEndOf="@+id/checkBox3" />

    <CheckBox
        android:id="@+id/checkBox5"
        android:layout_width="@dimen/celulaWidth"
        android:layout_height="@dimen/celulaHeight"
        android:background="@drawable/custom_selector"
        android:button="@null"
        android:clickable="false"
        app:layout_constraintBottom_toBottomOf="@+id/checkBox4"
        app:layout_constraintStart_toEndOf="@+id/checkBox4" />

    <CheckBox
        android:id="@+id/checkBox6"
        android:layout_width="@dimen/celulaWidth"
        android:layout_height="@dimen/celulaHeight"
        android:background="@drawable/custom_selector"
        android:button="@null"
        android:clickable="false"
        app:layout_constraintBottom_toBottomOf="@+id/checkBox5"
        app:layout_constraintStart_toEndOf="@+id/checkBox5" />

    <CheckBox
        android:id="@+id/checkBox7"
        android:layout_width="@dimen/celulaWidth"
        android:layout_height="@dimen/celulaHeight"
        android:background="@drawable/custom_selector"
        android:button="@null"
        android:clickable="false"
        app:layout_constraintBottom_toBottomOf="@+id/checkBox6"
        app:layout_constraintStart_toEndOf="@+id/checkBox6" />

</androidx.constraintlayout.widget.ConstraintLayout>